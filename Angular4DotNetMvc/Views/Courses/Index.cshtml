@model string
@{
  Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container" ng-init="courses = @Html.Raw(Model)">
  <div class="row">
    <div class="span10">
      <table class="table table-condensed table-hover">
        <thead>
          <tr>
            <th>Course</th>
            <th>Course Name</th>
            <th>Instructors</th>
          </tr>
        </thead>
        <tbody>
          <tr ng-repeat="course in courses">
            <td>{{course.number}}</td>
            <td>{{course.name}}</td>
            <td>{{course.instructor}}</td>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
</div>